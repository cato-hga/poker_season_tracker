<div class="container">
  <table class="table table-hover table-sm table-striped">
    <thead class="thead-inverse">
      <tr>
        <th>Name</th>
        <th>Games Attended</th>
        <th>Games Placed</th>
        <th>Attendance %</th>
        <th>Counted Score</th>
        <th>Actual Score</th>
      </tr>
    </thead>
    <tbody>
  <% @participants.each do |p| %>
  <tr>
    <td><%= p.display_name %></td>
    <td><%= p.total_game_count %></td>
    <td><%= p.game_top_three_count %></td>
    <td><%= p.percentage_attended %></td>
    <td><%= p.total_score %></td>
    <td><%= p.actual_score %></td>
  </tr>
  <% end %>
    </tbody>
  </table>
  </div>
